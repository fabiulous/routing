{
  "name": "@fabiulous/routing",
  "description": "Generates a typed routing mechanist based on a config",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/routing.umd.cjs",
